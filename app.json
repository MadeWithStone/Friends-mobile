{
  "expo": {
    "name": "Friends",
    "slug": "Friends-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "friends",
    "icon": "./assets/Friends-purple.png",
    "splash": {
      "image": "./assets/Friends-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.friendsmobile.friends",
      "buildNumber": "1.0.2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Friends-purple.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
